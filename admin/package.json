{
  "name": "gj-admin-dashboard",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "pm2": "pm2",
    "start": "node ./bin/www",
    "start:staging": "pm2 start",
    "monitor": "pm2 monit",
    "reload-env": "pm2 reload --update-env ecosystem.config.js",
    "stop": "pm2 stop ecosystem.config.js",
    "delete": "pm2 delete ecosystem.config.js",
    "knex": "knex --knexfile ./db/knexfile.js --debug"
  },
  "dependencies": {
    "acorn-jsx": "^5.0.1",
    "async-middleware": "^1.2.1",
    "bcrypt": "^3.0.6",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "browser-sync": "^2.26.7",
    "concurrently": "^4.1.1",
    "connect-redis": "^3.4.1",
    "cookie-parser": "^1.4.4",
    "debug": "^4.1.1",
    "del": "^4.1.1",
    "dotenv": "^8.0.0",
    "ejs": "^2.6.2",
    "express": "^4.17.1",
    "express-ejs-layouts": "^2.5.0",
    "express-session": "^1.16.2",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-cache": "^1.1.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-csscomb": "^3.0.8",
    "gulp-cssnano": "^2.1.3",
    "gulp-html-prettify": "0.0.1",
    "gulp-imagemin": "^6.0.0",
    "gulp-npm-dist": "^1.0.3",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-run": "^1.7.1",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "gulp-useref-plus": "0.0.8",
    "gulp-util": "^3.0.8",
    "gulp-wait": "0.0.2",
    "http-errors": "^1.7.3",
    "joi": "^14.3.1",
    "knex": "^0.18.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.11.0",
    "pino": "^5.12.6",
    "pm2": "^3.5.1",
    "postcss-flexbugs-fixes": "^4.1.0"
  },
  "devDependencies": {
    "eslint": "^6.0.1",
    "eslint-config": "^0.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "pino-pretty": "^3.2.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --config .prettierrc --write",
      "git add"
    ],
    "packages/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
